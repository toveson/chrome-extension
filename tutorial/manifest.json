{
    "manifest_version": 2,
    "name": "My Todo List",
    "version": "1.0.0",
    "description": "To-Do List",
    "icons": {
      "128": "assets/images/chrome128.png",
      "48": "assets/images/chrome48.png",
      "16": "assets/images/chrome16.png"
    },
  
    "browser_action": {
      "popup": "popup.html",
      "default_popup": "popup.html"
    },

    "permissions": [
        "storage"
    ]
  }