{
    "name": "color changer",
    "description": "Used to change background and text color",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "index.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "index.html"
    }

  }